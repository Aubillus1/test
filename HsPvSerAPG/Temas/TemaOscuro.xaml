<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!---Colores generales-->
    <SolidColorBrush Color="White" x:Key="SwitchTemaBackground"></SolidColorBrush>
    <SolidColorBrush Color="Black" x:Key="SwitchTemaCircle"></SolidColorBrush>
    <Color x:Key="ColorBackgroundSwitch">White</Color>

    <Style TargetType="TextBox">
        <Setter Property="CaretBrush" Value="White"/>
    </Style>

    <Style TargetType="PasswordBox">
        <Setter Property="CaretBrush" Value="White"/>
    </Style>

    <SolidColorBrush Color="#1D852A" x:Key="BotonAceptarBackground" />
    <SolidColorBrush Color="#53e965" x:Key="BotonAceptarBorder" />
    <Color x:Key="BtnAceptarSombra">#53e965</Color>

    <SolidColorBrush Color="#692525" x:Key="BtnCerrarBackground"></SolidColorBrush>
    <SolidColorBrush Color="#F77474" x:Key="BtnCerrarBorder"></SolidColorBrush>

    <RadialGradientBrush x:Key="GradientBackground" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.5,0.5">
        <GradientStop Color="#4A4848" Offset="0.0" />
        <GradientStop Color="Black" Offset="1.0" />
    </RadialGradientBrush>

    <LinearGradientBrush x:Key="LinearBackground" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="Black" Offset="0.0" />
        <GradientStop Color="#3B3B3B" Offset="1.0" />
    </LinearGradientBrush>
    <SolidColorBrush Color="LightGray" x:Key="Text"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="Text2"></SolidColorBrush>
    <SolidColorBrush Color="#424242" x:Key="TextBox"></SolidColorBrush>
    <SolidColorBrush Color="#5D748C" x:Key="TextBoxSombreado" />
    <SolidColorBrush Color="LightGray" x:Key="BordeTextBox"></SolidColorBrush>
    <Color x:Key="Sombras">White</Color>

    <SolidColorBrush Color="#517F9E" x:Key="BtnCurrentBackground"/>
    <SolidColorBrush Color="#96D2FA" x:Key="BtnCurrentBorder"/>

    <!---Colores POS-->
    <SolidColorBrush Color="#454545" x:Key="DataGrid"></SolidColorBrush>
    <SolidColorBrush Color="#ABABAB" x:Key="DataGridHeader"></SolidColorBrush>

    <SolidColorBrush Color="#c0c05c" x:Key="TBTotales"></SolidColorBrush>
    <SolidColorBrush Color="#d16d6d" x:Key="TBCant"></SolidColorBrush>

    <SolidColorBrush Color="DarkSlateGray" x:Key="BtnProductosBackground"></SolidColorBrush>
    <SolidColorBrush Color="#B4D1D1" x:Key="BtnProductosBorder"></SolidColorBrush>
    <SolidColorBrush Color="#c5613c" x:Key="BtnProductosEtiqueta"></SolidColorBrush>

    <SolidColorBrush Color="#333" x:Key="BtnGavetaBackground"></SolidColorBrush>
    <SolidColorBrush Color="#B1B1B5" x:Key="BtnGavetaBorder"></SolidColorBrush>

    <SolidColorBrush Color="#A11F24" x:Key="BtnSalirBackground"></SolidColorBrush>
    <SolidColorBrush Color="#ffc0c1" x:Key="BtnSalirBorder"></SolidColorBrush>
    <Color x:Key="SombraSalir">#ffc0c1</Color>


    <SolidColorBrush Color="#9376a7" x:Key="BtnReenviarBackground"></SolidColorBrush>
    <SolidColorBrush Color="#dbb8f4" x:Key="BtnReenviarBorder"></SolidColorBrush>
    <Color x:Key="SombraReenviar">#dbb8f4</Color>

    <SolidColorBrush Color="Black" x:Key="BtnLimpiarBackground"></SolidColorBrush>
    <SolidColorBrush Color="#B0B0B0" x:Key="BtnLimpiarBorder"></SolidColorBrush>
    <Color x:Key="SombraLimpiar">#B0B0B0</Color>

    <SolidColorBrush Color="#6c4aff" x:Key="BtnReemCajaBackground"></SolidColorBrush>
    <SolidColorBrush Color="#BDB0F5" x:Key="BtnReemCajaBorder"></SolidColorBrush>
    <Color x:Key="SombraReemCaja">#BDB0F5</Color>

    <SolidColorBrush Color="#D1AB64" x:Key="BtnCajaChicaBackground"></SolidColorBrush>
    <SolidColorBrush Color="#FFEBC4" x:Key="BtnCajaChicaBorder"></SolidColorBrush>
    <Color x:Key="SombraCajaChica">#FFEBC4</Color>

    <SolidColorBrush Color="#BAB259" x:Key="BtnCajaChicaOpcionesBackground"></SolidColorBrush>
    <SolidColorBrush Color="#FAF5B6" x:Key="BtnCajaChicaOpcionesForeground"></SolidColorBrush>
    <SolidColorBrush Color="#f5be00" x:Key="BtnCajaChicaIconoBackground"></SolidColorBrush>
    <SolidColorBrush Color="#fedc00" x:Key="BtnCajaChicaIcono2Background"></SolidColorBrush>

    <SolidColorBrush Color="Gray" x:Key="BtnClientesBackground"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="BtnClientesBorder"></SolidColorBrush>
    <Color x:Key="SombraClientes">White</Color>

    <SolidColorBrush Color="#9C8D4E" x:Key="BtnProductosPrincipalBackground"></SolidColorBrush>
    <SolidColorBrush Color="#F0E3A8" x:Key="BtnProductosPrincipalBorder"></SolidColorBrush>
    <Color x:Key="SombraProductosPrincipal">#F0E3A8</Color>

    <SolidColorBrush Color="#467C91" x:Key="BtnDocVentaBackground"></SolidColorBrush>
    <SolidColorBrush Color="#94D7F2" x:Key="BtnDocVentaBorder"></SolidColorBrush>
    <Color x:Key="SombraDocVenta">#94D7F2</Color>

    <LinearGradientBrush x:Key="ToolStrip" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#1E1E1E" Offset="0"/>
        <!-- Gris carbón -->
        <GradientStop Color="#2A2A2A" Offset="0.25"/>
        <!-- Gris oscuro -->
        <GradientStop Color="#3A3A3A" Offset="0.5"/>
        <!-- Gris medio -->
        <GradientStop Color="#2A2A2A" Offset="0.75"/>
        <!-- Gris oscuro -->
        <GradientStop Color="#1E1E1E" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HeaderPanelDerecho" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#2E2F66" Offset="0"/>
        <!-- Azul violeta oscuro -->
        <GradientStop Color="#2A3F55" Offset="0.5"/>
        <!-- Azul grisáceo profundo -->
        <GradientStop Color="#34596E" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush Color="#4D87AB" x:Key="BorderPanel"></SolidColorBrush>

    <LinearGradientBrush x:Key="HeaderSubPanel" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#611111" Offset="0"/>
        <GradientStop Color="#BF3030" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush Color="#F56C6C" x:Key="BorderSubPanel"></SolidColorBrush>


    <SolidColorBrush Color="#454545" x:Key="BackgroundPanel"></SolidColorBrush>

    <LinearGradientBrush x:Key="UMDBackground" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Color="#8C7A22" Offset="0"/>
        <GradientStop Color="#A68E2E" Offset="0.3"/>
        <GradientStop Color="#A68E2E" Offset="0.6"/>
        <GradientStop Color="#8C7A22" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush Color="#D9C250" x:Key="GridUMD"></SolidColorBrush>
    
    <!-- Colores Ventanas -->
    <SolidColorBrush Color="#6e2143" x:Key="BtnReniecBackground"/>
    <SolidColorBrush Color="#B86C8E" x:Key="BtnReniecBorder"/>

    <SolidColorBrush Color="#313A69" x:Key="BtnBuscarBackground"/>
    <SolidColorBrush Color="#8090E0" x:Key="BtnBuscarBorder"/>

    <SolidColorBrush Color="#8AA8A8" x:Key="BtnConsultarBackground"/>
    <SolidColorBrush Color="#CAFCFC" x:Key="BtnConsultarBorder"/>

    <SolidColorBrush Color="#86c938" x:Key="BtnPagoBackground"/>
    <SolidColorBrush Color="#C5F291" x:Key="BtnPagoBorder"/>

    <SolidColorBrush Color="#737632" x:Key="BtnTarjetaBackground"/>
    <SolidColorBrush Color="#f5f992" x:Key="BtnTarjetaBorder"/>

    <SolidColorBrush Color="#D1A164" x:Key="BtnBancoBackground"/>
    <SolidColorBrush Color="#FFB75C" x:Key="BtnBancoBorder"/>

    <SolidColorBrush Color="#174c23" x:Key="BtnBoletaBackground"/>
    <SolidColorBrush Color="#74B383" x:Key="BtnBoletaBorder"/>

    <SolidColorBrush Color="#1f2249" x:Key="BtnFacturaBackground"/>
    <SolidColorBrush Color="#99B7E8" x:Key="BtnFacturaBorder"/>

    <SolidColorBrush Color="#e20001" x:Key="BtnPDFBackground"/>
    <SolidColorBrush Color="White" x:Key="BtnPDFBorder"/>
    
    <SolidColorBrush Color="#B55F00" x:Key="IsTodayCalendar"/>
    <SolidColorBrush Color="White" x:Key="IsTodayCalendarText"/>
</ResourceDictionary>